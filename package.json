    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "^3.4.12"
  },
  "devDependencies": {
    "@babel/core": "7.22.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "1.21.1",
